alias s='git status'
alias b='git branch'
alias d='git diff'
alias ds='git diff --stat'
alias c='git commit'
alias cf='git commit -am "unnamed commit"'
alias gl='git log'
alias ck='git checkout'
alias gri='grep -Iri'

alias cd="pushd . > /dev/null; cd"

alias h="history | tac | view -"

alias vi="vi -p"
alias vp='view -'

alias l='ls'
alias ll='ls -l'
